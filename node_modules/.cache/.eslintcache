} from '../../logics/date/formateDate';\nimport Loader from '../Loader';\nimport { useDeleteArticleMutation } from '../../slices/deleteArticle';\nimport { callNotification } from '../../logics/errors/callLoginErrors';\nimport articleStyles from '../Article/Article.module.scss';\n\nimport styles from './FullArticle.module.scss';\n\nconst FullArticle: FC = () => {\n  const slug = useAppSelector((state) => state.article.slug) || '';\n  const [isOpenPopup, setIsOpenPopup] = useState<boolean>(false);\n  const [isPopupLoading, setIsPopupLoading] = useState<boolean>(false);\n  const navigate = useNavigate();\n\n  const isUserLoggedIn = useAppSelector(\n    (state) => state.userInfo.isUserLoggedIn,\n  );\n\n  const { data, error, isLoading } = useFullArticleQuery({ slug });\n  const [deleteArticle] = useDeleteArticleMutation();\n\n  const handleDelete = async () => {\n    try {\n      await deleteArticle(slug).unwrap();\n      callNotification('Article was deleted', 'success');\n      setTimeout(()=>{\n        navigate('/profile');\n      }, 2500);\n\n    } catch (err: any) {\n      if (err?.originalStatus === 403) {\n        callNotification(\n          'You are not the author to delete this article',\n          'error',\n        );\n        setTimeout(()=>{\n          navigate('/profile');\n        }, 2500);\n\n      }\n    }\n  };\n\n  const showPopconfirm = (): void => {\n    setIsOpenPopup(true);\n  };\n\n  const handleOk = (): void => {\n    setIsPopupLoading(true);\n\n    setTimeout((): void => {\n      setIsOpenPopup(false);\n\n      handleDelete();\n\n      setIsPopupLoading(false);\n    }, 2000);\n  };\n\n  const handleCancel = (): void => {\n    setIsOpenPopup(false);\n  };\n\n\n  return (\n    <>\n      {isLoading ? (\n        <Loader />\n      ) : (\n        <div className={styles.main}>\n          <div className={styles.container}>\n            <ToastContainer />\n            <header>\n              <div>\n                <div className={styles.headerFirstPart}>\n                  <h4 className={styles.title}>{data?.article?.title}</h4>\n                  <div className={articleStyles.likeBtn}>\n                    <Heart\n                      animationScale={1.1}\n                      onClick={() => null}\n                      isActive={false}\n                    />\n                  </div>\n                  <span>{data?.article?.favoritesCount}</span>\n                </div>\n                {data?.article?.tagList.map((tag) => (\n                  <span key={tag} className={styles.tag}>\n                    {tag}\n                  </span>\n                ))}\n              </div>\n              <div className={styles.user}>\n                <div>\n                  <h5 className={styles.userName}>\n                    {data?.article?.author.username}\n                  </h5>\n                  <span className={styles.date}>\n                    {formatDate(data?.article?.createdAt)}\n                  </span>\n                </div>\n\n                <img\n                  src={data?.article?.author.image}\n                  className={styles.userAvatar}\n                />\n              </div>\n            </header>\n            <div className={styles.descriptionContainer}>\n              <p className={styles.description}>{data?.article?.description}</p>\n              {isUserLoggedIn ? (\n                <div className={styles.articleEditButtons}>\n                  <Popconfirm\n                    title=\"Are you sure to delete this article?\"\n                    open={isOpenPopup}\n                    onConfirm={handleOk}\n                    okButtonProps={{ loading: isPopupLoading }}\n                    onCancel={handleCancel}\n                    className=\"ant-popover-content\"\n                  >\n                    <button\n                      className={styles.deleteButton}\n                      onClick={showPopconfirm}\n                    >\n                      Delete\n                    </button>\n                  </Popconfirm>\n                  <button className={styles.editButton}>Edit</button>\n                </div>\n              ) : (\n                ''\n              )}\n            </div>\n            <main className={styles.markdownContainer}>\n              <ReactMarkdown>{data?.article?.body || ''}</ReactMarkdown>\n            </main>\n          </div>\n        </div>\n      )}\n    </>\n  );\n};\n\nexport default FullArticle;\n","/Users/sanyadev/blog_app/src/slices/postAnArticle.ts",[],[],[],"/Users/sanyadev/blog_app/src/slices/deleteArticle.ts",[],[],"/Users/sanyadev/blog_app/src/slices/getUser.ts",[],[],"/Users/sanyadev/blog_app/src/slices/getArticlesFromApi.ts",[],[],"/Users/sanyadev/blog_app/src/slices/likeAnArticle.ts",[],[],"/Users/sanyadev/blog_app/src/slices/dislikeAnArticle.ts",[],[],{"ruleId":"202","severity":1,"message":"203","line":86,"column":21,"nodeType":"204","messageId":"205","endLine":86,"endColumn":24,"suggestions":"206"},{"ruleId":"202","severity":1,"message":"203","line":77,"column":21,"nodeType":"204","messageId":"205","endLine":77,"endColumn":24,"suggestions":"207"},{"ruleId":"208","severity":2,"message":"209","line":50,"column":5,"nodeType":"210","messageId":"211","endLine":50,"endColumn":18,"suppressions":"212"},{"ruleId":"208","severity":2,"message":"209","line":53,"column":5,"nodeType":"210","messageId":"211","endLine":53,"endColumn":18,"suppressions":"213"},{"ruleId":"208","severity":2,"message":"209","line":66,"column":5,"nodeType":"210","messageId":"211","endLine":66,"endColumn":18,"suppressions":"214"},{"ruleId":"202","severity":1,"message":"203","line":40,"column":19,"nodeType":"204","messageId":"205","endLine":40,"endColumn":22,"suggestions":"215"},{"ruleId":"216","severity":1,"message":"217","line":112,"column":17,"nodeType":"218","endLine":115,"endColumn":19},"@typescript-eslint/no-explicit-any","Unexpected any. Specify a different type.","TSAnyKeyword","unexpectedAny",["219","220"],["221","222